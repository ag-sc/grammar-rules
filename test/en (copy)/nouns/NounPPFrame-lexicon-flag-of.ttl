@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :flag-of ;
  lemon:entry    :flag-of_form_preposition .

:flag-of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :flag-of_form ;
  lemon:otherForm  :flag-of_singular ;
  lemon:otherForm  :flag-of_plural ;
  lemon:sense          :flag-of_sense_1 ;
  lemon:synBehavior    :flag-of_nounpp .

:flag-of_form a lemon:Form ;
  lemon:writtenRep "flag"@en .

:flag-of_singular a    lemon:Form ;
  lemon:writtenRep "flag"@en ;
  lexinfo:number   lexinfo:singular .

:flag-of_plural a   lemon:Form ;
  lemon:writtenRep "flags"@en ;
  lexinfo:number    lexinfo:plural .

:flag-of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:flag-of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :flag-of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/flag> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :flag-of_sense_1_condition .

:flag-of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :flag-of_form_preposition .
## Prepositions ##
:flag-of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

