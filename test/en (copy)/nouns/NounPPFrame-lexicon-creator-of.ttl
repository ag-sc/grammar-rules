@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :creator-of ;
  lemon:entry    :creator-of_form_preposition .

:creator-of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :creator-of_form ;
  lemon:otherForm  :creator-of_singular ;
  lemon:otherForm  :creator-of_plural ;
  lemon:sense          :creator-of_sense_1 ;
  lemon:synBehavior    :creator-of_nounpp .

:creator-of_form a lemon:Form ;
  lemon:writtenRep "creator"@en .

:creator-of_singular a    lemon:Form ;
  lemon:writtenRep "creator"@en ;
  lexinfo:number   lexinfo:singular .

:creator-of_plural a   lemon:Form ;
  lemon:writtenRep "creators"@en ;
  lexinfo:number    lexinfo:plural .

:creator-of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:creator-of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :creator-of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :creator-of_sense_1_condition .

:creator-of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/FictionalCharacter> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :creator-of_form_preposition .
## Prepositions ##
:creator-of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

