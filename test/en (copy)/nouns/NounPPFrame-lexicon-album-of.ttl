@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :album-of ;
  lemon:entry    :album-of_form_preposition .

:album-of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :album-of_form ;
  lemon:otherForm  :album-of_singular ;
  lemon:otherForm  :album-of_plural ;
  lemon:sense          :album-of_sense_1 ;
  lemon:synBehavior    :album-of_nounpp .

:album-of_form a lemon:Form ;
  lemon:writtenRep "album"@en .

:album-of_singular a    lemon:Form ;
  lemon:writtenRep "album"@en ;
  lexinfo:number   lexinfo:singular .

:album-of_plural a   lemon:Form ;
  lemon:writtenRep "albums"@en ;
  lexinfo:number    lexinfo:plural .

:album-of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:album-of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :album-of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/album> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :album-of_sense_1_condition .

:album-of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Album> .

:arg2 lemon:marker :album-of_form_preposition .
## Prepositions ##
:album-of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

