@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :city-in ;
  lemon:entry    :city-in_form_preposition .

:city-in a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :city-in_form ;
  lemon:otherForm  :city-in_singular ;
  lemon:otherForm  :city-in_plural ;
  lemon:sense          :city-in_sense_1 ;
  lemon:synBehavior    :city-in_nounpp .

:city-in_form a lemon:Form ;
  lemon:writtenRep "city"@en .

:city-in_singular a    lemon:Form ;
  lemon:writtenRep "city"@en ;
  lexinfo:number   lexinfo:singular .

:city-in_plural a   lemon:Form ;
  lemon:writtenRep "cities"@en ;
  lexinfo:number    lexinfo:plural .

:city-in_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:city-in_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :city-in_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/isPartOf> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :city-in_sense_1_condition .

:city-in_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/AdministrativeRegion> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :city-in_form_preposition .
## Prepositions ##
:city-in_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

