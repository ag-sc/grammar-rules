@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :andorran_sense_1_res ;
  lemon:entry    :andorran .

:andorran a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :andorran_lemma ;
  lemon:sense          :andorran_sense_1 ;
  lemon:synBehavior    :andorran_attrFrame, :andorran_predFrame .

:andorran_lemma lemon:writtenRep "andorran"@en .

:andorran_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :andorran_PredSynArg .

:andorran_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :andorran_AttrSynArg .

:andorran_sense_1 a  lemon:LexicalSense ;
  lemon:reference :andorran_sense_1_res ;
  lemon:isA       :andorran_AttrSynArg, :andorran_PredSynArg .

:andorran_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Andorra> .
