@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :azerbaijani_sense_1_res ;
  lemon:entry    :azerbaijani .

:azerbaijani a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :azerbaijani_lemma ;
  lemon:sense          :azerbaijani_sense_1 ;
  lemon:synBehavior    :azerbaijani_attrFrame, :azerbaijani_predFrame .

:azerbaijani_lemma lemon:writtenRep "azerbaijani"@en .

:azerbaijani_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :azerbaijani_PredSynArg .

:azerbaijani_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :azerbaijani_AttrSynArg .

:azerbaijani_sense_1 a  lemon:LexicalSense ;
  lemon:reference :azerbaijani_sense_1_res ;
  lemon:isA       :azerbaijani_AttrSynArg, :azerbaijani_PredSynArg .

:azerbaijani_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Azerbaijan> .
