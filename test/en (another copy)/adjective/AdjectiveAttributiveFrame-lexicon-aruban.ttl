@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :aruban_sense_1_res ;
  lemon:entry    :aruban .

:aruban a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :aruban_lemma ;
  lemon:sense          :aruban_sense_1 ;
  lemon:synBehavior    :aruban_attrFrame, :aruban_predFrame .

:aruban_lemma lemon:writtenRep "aruban"@en .

:aruban_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :aruban_PredSynArg .

:aruban_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :aruban_AttrSynArg .

:aruban_sense_1 a  lemon:LexicalSense ;
  lemon:reference :aruban_sense_1_res ;
  lemon:isA       :aruban_AttrSynArg, :aruban_PredSynArg .

:aruban_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Aruba> .
