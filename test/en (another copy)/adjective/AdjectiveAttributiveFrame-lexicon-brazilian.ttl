@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :brazilian_sense_1_res ;
  lemon:entry    :brazilian .

:brazilian a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :brazilian_lemma ;
  lemon:sense          :brazilian_sense_1 ;
  lemon:synBehavior    :brazilian_attrFrame, :brazilian_predFrame .

:brazilian_lemma lemon:writtenRep "brazilian"@en .

:brazilian_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :brazilian_PredSynArg .

:brazilian_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :brazilian_AttrSynArg .

:brazilian_sense_1 a  lemon:LexicalSense ;
  lemon:reference :brazilian_sense_1_res ;
  lemon:isA       :brazilian_AttrSynArg, :brazilian_PredSynArg .

:brazilian_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Brazil> .
