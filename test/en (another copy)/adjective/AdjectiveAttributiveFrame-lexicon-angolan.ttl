@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :angolan_sense_1_res ;
  lemon:entry    :angolan .

:angolan a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :angolan_lemma ;
  lemon:sense          :angolan_sense_1 ;
  lemon:synBehavior    :angolan_attrFrame, :angolan_predFrame .

:angolan_lemma lemon:writtenRep "angolan"@en .

:angolan_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :angolan_PredSynArg .

:angolan_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :angolan_AttrSynArg .

:angolan_sense_1 a  lemon:LexicalSense ;
  lemon:reference :angolan_sense_1_res ;
  lemon:isA       :angolan_AttrSynArg, :angolan_PredSynArg .

:angolan_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Angola> .
