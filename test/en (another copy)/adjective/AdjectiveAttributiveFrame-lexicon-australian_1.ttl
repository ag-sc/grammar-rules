@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :australian_1_sense_1_res ;
  lemon:entry    :australian_1 .

:australian_1 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :australian_1_lemma ;
  lemon:sense          :australian_1_sense_1 ;
  lemon:synBehavior    :australian_1_attrFrame, :australian_1_predFrame .

:australian_1_lemma lemon:writtenRep "australian"@en .

:australian_1_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :australian_1_PredSynArg .

:australian_1_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :australian_1_AttrSynArg .

:australian_1_sense_1 a  lemon:LexicalSense ;
  lemon:reference :australian_1_sense_1_res ;
  lemon:isA       :australian_1_AttrSynArg, :australian_1_PredSynArg .

:australian_1_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Australia> .
