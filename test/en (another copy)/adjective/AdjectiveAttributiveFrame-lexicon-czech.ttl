@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :czech_sense_1_res ;
  lemon:entry    :czech .

:czech a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :czech_lemma ;
  lemon:sense          :czech_sense_1 ;
  lemon:synBehavior    :czech_attrFrame, :czech_predFrame .

:czech_lemma lemon:writtenRep "czech"@en .

:czech_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :czech_PredSynArg .

:czech_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :czech_AttrSynArg .

:czech_sense_1 a  lemon:LexicalSense ;
  lemon:reference :czech_sense_1_res ;
  lemon:isA       :czech_AttrSynArg, :czech_PredSynArg .

:czech_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Czech_Republic> .
