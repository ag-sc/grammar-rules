@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :bengali_sense_1_res ;
  lemon:entry    :bengali .

:bengali a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :bengali_lemma ;
  lemon:sense          :bengali_sense_1 ;
  lemon:synBehavior    :bengali_attrFrame, :bengali_predFrame .

:bengali_lemma lemon:writtenRep "bengali"@en .

:bengali_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :bengali_PredSynArg .

:bengali_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :bengali_AttrSynArg .

:bengali_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bengali_sense_1_res ;
  lemon:isA       :bengali_AttrSynArg, :bengali_PredSynArg .

:bengali_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Bangladesh> .
