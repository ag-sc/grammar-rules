@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :algerian_sense_1_res ;
  lemon:entry    :algerian .

:algerian a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :algerian_lemma ;
  lemon:sense          :algerian_sense_1 ;
  lemon:synBehavior    :algerian_attrFrame, :algerian_predFrame .

:algerian_lemma lemon:writtenRep "algerian"@en .

:algerian_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :algerian_PredSynArg .

:algerian_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :algerian_AttrSynArg .

:algerian_sense_1 a  lemon:LexicalSense ;
  lemon:reference :algerian_sense_1_res ;
  lemon:isA       :algerian_AttrSynArg, :algerian_PredSynArg .

:algerian_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Algeria> .
