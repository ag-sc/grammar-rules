@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :belizean_sense_1_res ;
  lemon:entry    :belizean .

:belizean a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :belizean_lemma ;
  lemon:sense          :belizean_sense_1 ;
  lemon:synBehavior    :belizean_attrFrame, :belizean_predFrame .

:belizean_lemma lemon:writtenRep "belizean"@en .

:belizean_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :belizean_PredSynArg .

:belizean_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :belizean_AttrSynArg .

:belizean_sense_1 a  lemon:LexicalSense ;
  lemon:reference :belizean_sense_1_res ;
  lemon:isA       :belizean_AttrSynArg, :belizean_PredSynArg .

:belizean_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Belize> .
