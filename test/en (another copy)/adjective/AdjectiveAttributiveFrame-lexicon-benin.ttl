@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :benin_sense_1_res ;
  lemon:entry    :benin .

:benin a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :benin_lemma ;
  lemon:sense          :benin_sense_1 ;
  lemon:synBehavior    :benin_attrFrame, :benin_predFrame .

:benin_lemma lemon:writtenRep "benin"@en .

:benin_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :benin_PredSynArg .

:benin_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :benin_AttrSynArg .

:benin_sense_1 a  lemon:LexicalSense ;
  lemon:reference :benin_sense_1_res ;
  lemon:isA       :benin_AttrSynArg, :benin_PredSynArg .

:benin_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Benin> .
