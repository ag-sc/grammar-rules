@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :bolivian_sense_1_res ;
  lemon:entry    :bolivian .

:bolivian a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :bolivian_lemma ;
  lemon:sense          :bolivian_sense_1 ;
  lemon:synBehavior    :bolivian_attrFrame, :bolivian_predFrame .

:bolivian_lemma lemon:writtenRep "bolivian"@en .

:bolivian_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :bolivian_PredSynArg .

:bolivian_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :bolivian_AttrSynArg .

:bolivian_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bolivian_sense_1_res ;
  lemon:isA       :bolivian_AttrSynArg, :bolivian_PredSynArg .

:bolivian_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Bolivia> .
