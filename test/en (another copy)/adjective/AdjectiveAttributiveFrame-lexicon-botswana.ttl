@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :botswana_sense_1_res ;
  lemon:entry    :botswana .

:botswana a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :botswana_lemma ;
  lemon:sense          :botswana_sense_1 ;
  lemon:synBehavior    :botswana_attrFrame, :botswana_predFrame .

:botswana_lemma lemon:writtenRep "botswana"@en .

:botswana_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :botswana_PredSynArg .

:botswana_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :botswana_AttrSynArg .

:botswana_sense_1 a  lemon:LexicalSense ;
  lemon:reference :botswana_sense_1_res ;
  lemon:isA       :botswana_AttrSynArg, :botswana_PredSynArg .

:botswana_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Botswana> .
