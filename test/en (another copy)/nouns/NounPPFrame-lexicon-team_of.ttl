@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :team_of ;
  lemon:entry    :team_of_form_preposition .

:team_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :team_of_form ;
  lemon:otherForm  :team_of_singular ;
  lemon:otherForm  :team_of_plural ;
  lemon:sense          :team_of_sense_1 ;
  lemon:synBehavior    :team_of_nounpp .

:team_of_form a lemon:Form ;
  lemon:writtenRep "team"@en .

:team_of_singular a    lemon:Form ;
  lemon:writtenRep "team"@en ;
  lexinfo:number   lexinfo:singular .

:team_of_plural a   lemon:Form ;
  lemon:writtenRep "teams"@en ;
  lexinfo:number    lexinfo:plural .

:team_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:team_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :team_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/league> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :team_of_sense_1_condition .

:team_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/HockeyTeam> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/IceHockeyLeague> .

:arg2 lemon:marker :team_of_form_preposition .
## Prepositions ##
:team_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

