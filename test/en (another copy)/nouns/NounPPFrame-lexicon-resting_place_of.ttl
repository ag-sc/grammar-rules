@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :resting_place_of ;
  lemon:entry    :resting_place_of_form_preposition .

:resting_place_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :resting_place_of_form ;
  lemon:otherForm  :resting_place_of_singular ;
  lemon:otherForm  :resting_place_of_plural ;
  lemon:sense          :resting_place_of_sense_1 ;
  lemon:synBehavior    :resting_place_of_nounpp .

:resting_place_of_form a lemon:Form ;
  lemon:writtenRep "resting place"@en .

:resting_place_of_singular a    lemon:Form ;
  lemon:writtenRep "resting place"@en ;
  lexinfo:number   lexinfo:singular .

:resting_place_of_plural a   lemon:Form ;
  lemon:writtenRep "resting places"@en ;
  lexinfo:number    lexinfo:plural .

:resting_place_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:resting_place_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :resting_place_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/restingPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :resting_place_of_sense_1_condition .

:resting_place_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :resting_place_of_form_preposition .
## Prepositions ##
:resting_place_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

