@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Book-in ;
  lemon:entry    :Book-in_form_preposition .

:Book-in a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Book-in_form ;
  lemon:otherForm  :Book-in_singular ;
  lemon:otherForm  :Book-in_plural ;
  lemon:sense          :Book-in_sense_1 ;
  lemon:synBehavior    :Book-in_nounpp .

:Book-in_form a lemon:Form ;
  lemon:writtenRep "book"@en .

:Book-in_singular a    lemon:Form ;
  lemon:writtenRep "book"@en ;
  lexinfo:number   lexinfo:singular .

:Book-in_plural a   lemon:Form ;
  lemon:writtenRep "books"@en ;
  lexinfo:number    lexinfo:plural .

:Book-in_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:Book-in_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Book-in_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/series> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Book-in_sense_1_condition .

:Book-in_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/TelevisionShow> .

:arg2 lemon:marker :Book-in_form_preposition .
## Prepositions ##
:Book-in_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

