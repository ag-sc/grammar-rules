@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :firstAscentPerson-of ;
  lemon:entry    :firstAscentPerson-of_form_preposition .

:firstAscentPerson-of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :firstAscentPerson-of_form ;
  lemon:otherForm  :firstAscentPerson-of_singular ;
  lemon:otherForm  :firstAscentPerson-of_plural ;
  lemon:sense          :firstAscentPerson-of_sense_1 ;
  lemon:synBehavior    :firstAscentPerson-of_nounpp .

:firstAscentPerson-of_form a lemon:Form ;
  lemon:writtenRep "first Ascent Person"@en .

:firstAscentPerson-of_singular a    lemon:Form ;
  lemon:writtenRep "first Ascent Person"@en ;
  lexinfo:number   lexinfo:singular .

:firstAscentPerson-of_plural a   lemon:Form ;
  lemon:writtenRep "first Ascent Persons"@en ;
  lexinfo:number    lexinfo:plural .

:firstAscentPerson-of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:firstAscentPerson-of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :firstAscentPerson-of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/firstAscentPerson> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :firstAscentPerson-of_sense_1_condition .

:firstAscentPerson-of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :firstAscentPerson-of_form_preposition .
## Prepositions ##
:firstAscentPerson-of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

